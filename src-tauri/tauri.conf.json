{
    "package": {
        "productName": "Tinker 2",
        "version": "1.0.0"
    },
    "build": {
        "distDir": "../build",
        "devPath": "http://localhost:3000",
        "beforeDevCommand": "yarn dev",
        "beforeBuildCommand": "yarn build"
    },
    "tauri": {
        "cli": {
            "description": "Launch at current directory",
            "longDescription": null,
            "beforeHelp": null,
            "afterHelp": null,
            "args": [
                {
                    "name": "directory",
                    "description": "working directory",
                    "takesValue": true,
                    "index": 1
                }
            ]
        },
        "bundle": {
            "active": true,
            "targets": [
                "all"
            ],
            "identifier": "com.daudau.tinker2",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "resources": [
                "bin/psycho.phar"
            ],
            "externalBin": [],
            "copyright": "",
            "category": "DeveloperTool",
            "shortDescription": "",
            "longDescription": "",
            "deb": {
                "depends": []
            },
            "macOS": {
                "frameworks": [],
                "minimumSystemVersion": "",
                "exceptionDomain": "",
                "signingIdentity": null,
                "entitlements": null
            },
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "updater": {
            "active": true,
            "endpoints": [
                "https://lab.daudau.cc/api/update/tinker-2/{{target}}/{{current_version}}"
            ],
            "dialog": true,
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEEzMjVBM0M3QzRBMTBEM0YKUldRL0RhSEV4Nk1sbzc4N3QvVkZrQVlBVVNBOGo5SFd5NkFkK2l6K3BidFliTDlnSkdxcFpLSmUK"
        },
        "allowlist": {
            "all": false,
            "fs": {
                "all": false,
                "readFile": true,
                "writeFile": true,
                "readDir": true,
                "createDir": true,
                "scope": [
                    "**/*"
                ]
            },
            "shell": {
                "all": true,
                "scope": [
                    {
                        "name": "ssh",
                        "cmd": "ssh",
                        "args": true
                    },
                    {
                        "name": "scp",
                        "cmd": "scp",
                        "args": true
                    }
                ]
            },
            "dialog": {
                "open": true,
                "all": false,
                "save": false
            },
            "path": {
                "all": true
            },
            "http": {
                "all": true,
                "request": true,
                "scope": [
                    "https://**",
                    "https://**"
                ]
            }
        },
        "windows": [
            {
                "title": "Tinker 2",
                "width": 1200,
                "height": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
        }
    }
}